	note = {Place: England},
	keywords = {Animals, Bioconcentration, Dibenzofurans, Polychlorinated/analysis, Dibenzofurans/analysis, Eggs, Eggs/analysis, Environmental Monitoring, Environmental Pollutants/*analysis, Food Chain, Food Contamination/*analysis, Halogenated Diphenyl Ethers/analysis, Livestock, Milk, Milk/chemistry, Persistent organic pollutants, Polychlorinated Biphenyls/analysis, Polychlorinated Dibenzodioxins/analysis, Tissues, Transfer},
