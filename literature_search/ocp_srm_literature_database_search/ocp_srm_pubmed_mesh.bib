	note = {Place: England},
	keywords = {Clinical Protocols, Condylomata Acuminata/*economics/therapy, Cost-Benefit Analysis, Cryotherapy/economics, Fees, Medical, Female, Follow-Up Studies, Genital Diseases, Female/*economics/therapy, Genital Diseases, Male/*economics/therapy, Health Care Costs, Humans, Insurance, Health, Reimbursement, Keratolytic Agents/economics/therapeutic use, Laser Therapy/economics, Male, Models, Economic, Podophyllin/economics/therapeutic use, Podophyllotoxin/economics/therapeutic use, Recurrence, Relative Value Scales, Remission Induction, Sex Factors, Trichloroacetic Acid/economics/therapeutic use},
