	note = {Place: United States},
	keywords = {*Environmental Exposure, Carcinogens/*toxicity, Case-Control Studies, Cohort Studies, Food Contamination, Humans, Incidence, Neoplasms/chemically induced/*epidemiology/mortality, Occupational Exposure, Polychlorinated Biphenyls/blood/*toxicity},
	pages = {99--144},
}

