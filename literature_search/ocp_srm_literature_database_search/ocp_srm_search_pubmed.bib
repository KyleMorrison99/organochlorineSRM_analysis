@article{montano_persistent_2013,
	title = {Persistent toxic burdens of halogenated phenolic compounds in humans and wildlife.},
	volume = {47},
	issn = {1520-5851 0013-936X},
	doi = {10.1021/es400478k},
	abstract = {Halogenated phenolic compounds (HPCs) including hydroxylated polychlorobiphenyls (OH-PCBs) and hydroxylated polybromodiphenyl-ethers (OH-PBDEs) can be persistent  organic pollutant (POP) metabolites or natural marine compounds. Structurally  similar to thyroid hormones (THs), they are retained in blood, transported  through selective barriers, and the cause of endocrine and neuronal POP effects.  This study presents a meta-analysis of HPC burdens in human and wildlife tissues,  including OH-PCBs, OH-PBDEs, Pentachlorophenol, and polybromophenols. HPC blood  plasma levels were also compared to known in vitro and in vivo toxicological  effect concentrations. Blood, highly perfused, and fetal tissues contained the  highest levels of HPCs. Plasma concentrations of analyzed OH-PCBs/PBDEs ranged  from 0.1 to 100 nM in humans and up to 240, 454, 800, and 7650 nM for birds,  fish, cetaceans, and other mammals, respectively. These concentrations fully fall  within the in vitro effect concentrations reported in literature for HPCs of  0.05-10000 nM. We strongly advise further study of HPC blood levels in the  general population, children, and fetal tissue to establish background levels and  the risk at sensitive development stages. As not all HPCs are, or can be,  chemically analyzed, the application of additional bioanalysis might reveal an  even greater toxicological relevance of HPCs. In addition, metabolic activation  should always be included within in vitro hazard assessment of POPs.},
	language = {eng},
	number = {12},
	journal = {Environmental science \& technology},
	author = {Montaño, Mauricio and Gutleb, Arno C. and Murk, Albertinka J.},
	month = jun,
	year = {2013},
	pmid = {23635024},
	note = {Place: United States},
	keywords = {Humans, Animals, Environmental Monitoring, Environmental Pollutants/metabolism/*toxicity, Phenols/metabolism/*toxicity, Polychlorinated Biphenyls/metabolism/toxicity},
	pages = {6071--6081},
}

@article{patterson_skin_2016,
	title = {Skin diseases associated with {Agent} {Orange} and other organochlorine exposures.},
	volume = {74},
	copyright = {Copyright © 2015 American Academy of Dermatology, Inc. All rights reserved.},
	issn = {1097-6787 0190-9622},
	doi = {10.1016/j.jaad.2015.05.006},
	abstract = {Organochlorine exposure is an important cause of cutaneous and systemic toxicity. Exposure has been associated with industrial accidents, intentional poisoning,  and the use of defoliants, such as Agent Orange in the Vietnam War. Although  long-term health effects are systematically reviewed by the Institute of  Medicine, skin diseases are not comprehensively assessed. This represents an  important practice gap as patients can present with cutaneous findings. This  article provides a systematic review of the cutaneous manifestations of known  mass organochlorine exposures in military and industrial settings with the goal  of providing clinically useful recommendations for dermatologists seeing patients  inquiring about organochlorine effects. Patients with a new diagnosis of  chloracne, porphyria cutanea tarda, cutaneous lymphomas (non-Hodgkin lymphoma),  and soft-tissue sarcomas including dermatofibrosarcoma protuberans and  leiomyosarcomas should be screened for a history of Vietnam service or industrial  exposure. Inconclusive evidence exists for an increased risk of other skin  diseases in Vietnam veterans exposed to Agent Orange including benign fatty  tumors, melanomas, nonmelanoma skin cancers, milia, eczema, dyschromias,  disturbance of skin sensation, and rashes not otherwise specified. Affected  veterans should be informed of the uncertain data in those cases. Referral to  Department of Veterans Affairs for disability assessment is indicated for  conditions with established associations.},
	language = {eng},
	number = {1},
	journal = {Journal of the American Academy of Dermatology},
	author = {Patterson, Andrew T. and Kaffenberger, Benjamin H. and Keller, Richard A. and Elston, Dirk M.},
	month = jan,
	year = {2016},
	pmid = {26210237},
	note = {Place: United States},
	keywords = {Humans, Male, Female, organochlorine, United States, Risk Assessment, Incidence, veteran, Follow-Up Studies, skin disease, chloracne, Vietnam, Environmental Exposure/*adverse effects, Hydrocarbons, Chlorinated/*adverse effects, *Military Personnel, 2,3,7,8-tetrachlorodibenzo-p-dioxin, 2,4-Dichlorophenoxyacetic Acid/*adverse effects, 2,4,5-Trichlorophenoxyacetic Acid/*adverse effects, Agent Orange, dioxin exposure, Polychlorinated Dibenzodioxins/*adverse effects, Skin Diseases/*chemically induced/epidemiology/physiopathology},
