	keywords = {Male, Animals, Ecosystem, Female, Pregnancy, Marine Biology, Environmental Monitoring/*methods, Mammals/*metabolism, Water Pollutants, Chemical/*pharmacokinetics, *International Agencies, Hydrocarbons, Chlorinated/pharmacokinetics, Metals/pharmacokinetics},
	pages = {249--256},
