}

@article{simpson_assessment_1990,
	title = {Assessment of fat absorption in normal dogs and in dogs with hyperadrenocorticalism.},
	volume = {48},
	issn = {0034-5288},
	abstract = {Fat absorption was determined quantitatively in clinically normal dogs, dogs with confirmed hyperadrenocorticalism before and after treatment with mitotane (op'  DDD) and in pruritic dogs before and after administration of prednisolone. Fat  absorption was significantly higher (P less than 0.001) in dogs with untreated  hyperadrenocorticalism and pruritic dogs on prednisolone therapy than in normal  dogs. It was normal in pruritic dogs before prednisolone therapy and approached  normality in dogs with treated hyperadrenocorticalism. It is concluded that high  circulating cortisol or prednisolone levels result in increased intestinal  absorption of dietary fat possibly mediated by increased glucocorticoid induced  activity of enterocytes or reduced hepatic clearance of triglycerides.},
