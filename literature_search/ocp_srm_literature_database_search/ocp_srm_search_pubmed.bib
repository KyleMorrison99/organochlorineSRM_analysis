	note = {Place: Netherlands},
	keywords = {*Food Chain, Animals, Arctic Regions, Biological Availability, Diet, Hydrocarbons, Chlorinated/chemistry/*pharmacokinetics, Pesticides/chemistry/*pharmacokinetics, Tissue Distribution, Water Movements, Water Pollutants, Chemical/*pharmacokinetics},
	pages = {87--106},
}

