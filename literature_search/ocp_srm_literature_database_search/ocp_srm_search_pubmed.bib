	note = {Place: Netherlands},
	keywords = {Insecticides/chemistry/*toxicity, Superoxide Dismutase/metabolism, Hydrogen Peroxide/metabolism, Anabaena variabilis/drug effects/*physiology, Ascorbate Peroxidases/metabolism, Catalase/pharmacology, Cholinesterase Inhibitors/chemistry/toxicity, Glycogen/chemistry, Malathion/chemistry/*toxicity, Malondialdehyde/metabolism, Mannitol/chemistry, Proline/chemistry, Sucrose/chemistry, Trehalose/chemistry},
