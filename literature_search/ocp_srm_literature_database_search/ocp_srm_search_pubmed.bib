	keywords = {Male, Animals, Sensitivity and Specificity, Tissue Distribution, Gas Chromatography-Mass Spectrometry, Rats, Sprague-Dawley, Endocrine System/drug effects, Endosulfan/*blood/*metabolism, Insecticides/*blood/*metabolism, Rats/blood},
