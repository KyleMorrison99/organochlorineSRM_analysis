	note = {Place: United States},
	keywords = {Industry, Industrial Waste, Dibenzofurans, Polychlorinated, Environmental Monitoring/methods, Polychlorinated Biphenyls/*analysis, Benzofurans/*analysis, Polychlorinated Dibenzodioxins/*analogs \& derivatives/analysis, Air Pollutants/*analysis, *Coke, Chlorobenzenes/*analysis, Environmental Pollutants/analysis, Fungicides, Industrial/analysis, Hexachlorobenzene/*analysis},
