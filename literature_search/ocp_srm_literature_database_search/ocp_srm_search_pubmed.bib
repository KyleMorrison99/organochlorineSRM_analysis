	keywords = {Humans, Child, Phenotype, *autism spectrum disorder, *neurodevelopment, Genetic Association Studies, *Intellectual Disability/genetics, *Neurodevelopmental Disorders/genetics, *TLK2, Developmental Disabilities/genetics/pathology, Heterozygote, Whole Exome Sequencing},
