	keywords = {Europe, Animals, Air Pollutants, Antarctic Regions, Water Pollutants, Chemical/*analysis, Chromatography, Gas, Water Pollutants/*analysis, Fishes/metabolism, Hydrocarbons, Halogenated/*analysis, Oils/analysis},
