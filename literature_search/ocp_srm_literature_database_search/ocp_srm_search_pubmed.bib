	note = {Place: Ireland},
	keywords = {Male, Animals, Rats, Carcinogens, Drug Interactions, Isomerism, Enzyme Induction, Dose-Response Relationship, Drug, Hexachlorocyclohexane/*toxicity, Cytochrome P-450 Enzyme System/metabolism, Cytochrome P-450 CYP1A1/metabolism, Organ Size/drug effects, Rats, Inbred F344, Steroid Hydroxylases/metabolism, Diethylnitrosamine, Cytochrome P-450 CYP2B1/metabolism, Glutathione Transferase/*metabolism, Biomarkers, Tumor/*metabolism, Liver Neoplasms/*chemically induced/enzymology, Liver/drug effects/enzymology, Precancerous Conditions/*chemically induced/enzymology},
	pages = {179--185},
}

@article{jimenez_use_2007,
	title = {Use of {SPE}-{GC}/{EIMS} for residue analysis in wine elaborated from musts spiked with formulations of chlorpyriphos-methyl, methiocarb, dicofol, and cyproconazol.},
	volume = {30},
	issn = {1615-9306},
	doi = {10.1002/jssc.200600345},
	abstract = {The likely presence in wine of residues of the active ingredient and its degradation products, besides the byproducts and excipients of the commercial  formulation, has been investigated for four pesticides. Formulations containing  chlorpyriphos-methyl, methiocarb, dicofol, and cyproconazol were added to must,  which was subjected to a usual vinification. The wines elaborated from must  spiked with the formulation of chlorpyriphos-methyl contained two pyridinol  compounds in addition to excipients such as alkylbenzenes, naphthalene, and  methylnaphthalenes. Methiocarb was hydrolyzed to yield the corresponding phenol,  and various unidentified compounds related to cyproconazol were observed in wine.  The residues of the dicofol-containing formulation resulted to be dechlorination  products; impurities from its commercial formulation were also detected in must  and wine extracts. White wines contained higher amounts of residues than red  wines. The residues were detected after an SPE followed by GC/EIMS in the scan  mode. The concentrations of the active ingredients were determined by a  matrix-matched calibration to avoid quantitative errors arising from the matrix.},
	language = {eng},
	number = {4},
	journal = {Journal of separation science},
	author = {Jiménez, Juan José and Bernal, José Luis and del Nozal, María J. and Toribio, Laura and Bernal, José},
	month = mar,
