	keywords = {Phenotype, Sequence Deletion, Chlorophenols/*toxicity, Pesticides/metabolism, *Genome, Fungal, Amino Acids, Aromatic/*biosynthesis, Disinfectants/metabolism, Environmental Pollutants/metabolism/toxicity, Herbicides/metabolism, Mitochondrial Proteins/*biosynthesis, Saccharomyces cerevisiae/*drug effects/genetics},
	pages = {81--91},
