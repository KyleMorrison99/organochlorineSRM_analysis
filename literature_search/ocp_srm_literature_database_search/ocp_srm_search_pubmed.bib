	pmid = {32319732},
	note = {Place: United States},
	keywords = {Humans, Male, Female, Infant, Pregnancy, Child, Adolescent, Child, Preschool, Syndrome, *intellectual disability, * HNRNPU, *DDD study, *exome sequencing, *seizures, Brain/diagnostic imaging, Craniofacial Abnormalities/etiology, Haploinsufficiency/genetics, Heterogeneous-Nuclear Ribonucleoprotein U/*genetics, Intellectual Disability/genetics, Microcephaly/etiology, Neurodevelopmental Disorders/*etiology/genetics, Seizures/genetics},
