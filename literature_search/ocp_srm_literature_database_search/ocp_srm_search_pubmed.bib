	keywords = {Humans, Animals, Aedes albopictus, Mutation, Alleles, Insecticides/*pharmacology, *Insecticide Resistance, kdr, Insecticide resistance, Aedes/drug effects/*genetics, Allele-specific PCR, Dengue/*transmission, Insect Proteins/*genetics, Mosquito Vectors/drug effects/*genetics/metabolism, Polymerase Chain Reaction/*methods},
	pages = {603},
