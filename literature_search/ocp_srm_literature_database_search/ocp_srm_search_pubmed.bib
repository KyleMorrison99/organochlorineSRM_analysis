	keywords = {Male, Animals, In Vitro Techniques, Temperature, Radioimmunoassay, Dithiothreitol/pharmacology, Iodide Peroxidase/*analysis, Kidney/drug effects/*enzymology, Propylthiouracil/pharmacology, Rana ridibunda, Thyroxine/metabolism, Triiodothyronine/biosynthesis},
