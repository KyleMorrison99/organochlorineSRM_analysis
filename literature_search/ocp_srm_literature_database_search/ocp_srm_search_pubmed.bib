	note = {Place: China},
	keywords = {Humans, Male, Adult, Gene Expression Regulation, Reverse Transcriptase Polymerase Chain Reaction, Molecular Sequence Data, Organ Specificity, Cloning, Molecular, Sequence Alignment, DNA, Complementary/genetics, Testis/*metabolism, Databases, Genetic, Homeodomain Proteins/*genetics, Proteins/*genetics, RNA, Long Noncoding},
