	pages = {247--262},
}

@article{bisaillon_quantitative_2011,
	title = {Quantitative analysis of the relative transcript levels of four chlorophenol reductive dehalogenase genes in {Desulfitobacterium} hafniense {PCP}-1 exposed to  chlorophenols.},
