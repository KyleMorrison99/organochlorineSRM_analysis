@article{olson_recurrent_2018,
	title = {A {Recurrent} {De} {Novo} {PACS2} {Heterozygous} {Missense} {Variant} {Causes} {Neonatal}-{Onset} {Developmental} {Epileptic} {Encephalopathy}, {Facial} {Dysmorphism}, and {Cerebellar}  {Dysgenesis}.},
	volume = {102},
	copyright = {Copyright © 2018 American Society of Human Genetics. All rights reserved.},
	issn = {1537-6605 0002-9297},
	doi = {10.1016/j.ajhg.2018.03.005},
	abstract = {Developmental and epileptic encephalopathies (DEEs) represent a large clinical and genetic heterogeneous group of neurodevelopmental diseases. The  identification of pathogenic genetic variants in DEEs remains crucial for  deciphering this complex group and for accurately caring for affected individuals  (clinical diagnosis, genetic counseling, impacting medical, precision therapy,  clinical trials, etc.). Whole-exome sequencing and intensive data sharing  identified a recurrent de novo PACS2 heterozygous missense variant in 14  unrelated individuals. Their phenotype was characterized by epilepsy, global  developmental delay with or without autism, common cerebellar dysgenesis, and  facial dysmorphism. Mixed focal and generalized epilepsy occurred in the neonatal  period, controlled with difficulty in the first year, but many improved in early  childhood. PACS2 is an important PACS1 paralog and encodes a multifunctional  sorting protein involved in nuclear gene expression and pathway traffic  regulation. Both proteins harbor cargo(furin)-binding regions (FBRs) that bind  cargo proteins, sorting adaptors, and cellular kinase. Compared to the defined  PACS1 recurrent variant series, individuals with PACS2 variant have more  consistently neonatal/early-infantile-onset epilepsy that can be challenging to  control. Cerebellar abnormalities may be similar but PACS2 individuals exhibit a  pattern of clear dysgenesis ranging from mild to severe. Functional studies  demonstrated that the PACS2 recurrent variant reduces the ability of the  predicted autoregulatory domain to modulate the interaction between the PACS2 FBR  and client proteins, which may disturb cellular function. These findings support  the causality of this recurrent de novo PACS2 heterozygous missense in DEEs with  facial dysmorphim and cerebellar dysgenesis.},
	language = {eng},
	number = {5},
	journal = {American journal of human genetics},
	author = {Olson, Heather E. and Jean-Marçais, Nolwenn and Yang, Edward and Heron, Delphine and Tatton-Brown, Katrina and van der Zwaag, Paul A. and Bijlsma, Emilia K. and Krock, Bryan L. and Backer, E. and Kamsteeg, Erik-Jan and Sinnema, Margje and Reijnders, Margot R. F. and Bearden, David and Begtrup, Amber and Telegrafi, Aida and Lunsing, Roelineke J. and Burglen, Lydie and Lesca, Gaetan and Cho, Megan T. and Smith, Lacey A. and Sheidley, Beth R. and Moufawad El Achkar, Christelle and Pearl, Phillip L. and Poduri, Annapurna and Skraban, Cara M. and Tarpinian, Jennifer and Nesbitt, Addie I. and Fransen van de Putte, Dietje E. and Ruivenkamp, Claudia A. L. and Rump, Patrick and Chatron, Nicolas and Sabatier, Isabelle and De Bellescize, Julitta and Guibaud, Laurent and Sweetser, David A. and Waxler, Jessica L. and Wierenga, Klaas J. and Donadieu, Jean and Narayanan, Vinodh and Ramsey, Keri M. and Nava, Caroline and Rivière, Jean-Baptiste and Vitobello, Antonio and Tran Mau-Them, Frédéric and Philippe, Christophe and Bruel, Ange-Line and Duffourd, Yannis and Thomas, Laurel and Lelieveld, Stefan H. and Schuurs-Hoeijmakers, Janneke and Brunner, Han G. and Keren, Boris and Thevenon, Julien and Faivre, Laurence and Thomas, Gary and Thauvin-Robinet, Christel},
	month = may,
	year = {2018},
	pmid = {29656858},
	pmcid = {PMC5986694},
	keywords = {Humans, Male, Female, Infant, Phenotype, Age of Onset, Infant, Newborn, Child, Preschool, *epilepsy, Heterozygote, *intellectual disability, Mutation, Missense/*genetics, *cerebellar dysgenesis, *Facies, *PACS-2, *PACS2, Cerebellar Diseases/*genetics, Epilepsy, Generalized/*genetics, Vesicular Transport Proteins/*genetics},
	pages = {995--1007},
