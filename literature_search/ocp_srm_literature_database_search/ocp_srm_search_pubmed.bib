	note = {Place: England},
	keywords = {Animals, Environmental Monitoring, Mexico, Tissue Distribution, Adipose Tissue/chemistry, Liver/chemistry, Environmental Pollutants/analysis/*pharmacokinetics, Insecticides/analysis/*pharmacokinetics, Polychlorinated Biphenyls/analysis/*pharmacokinetics, Water Pollutants, Chemical/analysis/*pharmacokinetics, Kidney/chemistry, *Turtles, *Developing Countries, Pesticide Residues/analysis/*pharmacokinetics},
	pages = {1082--1089},
}

@article{andersen_high-pressure_1995,
	title = {A high-pressure liquid chromatographic method for measuring mitotane [1,1-(o,p'-{Dichlorodiphenyl})-2,2-dichloroethane] and its metabolite  1,1-(o,p'-{Dichlorodiphenyl})-2,2-dichloroethene in plasma.},
	volume = {17},
	issn = {0163-4356},
	doi = {10.1097/00007691-199510000-00015},
	abstract = {The adrenolytic agent mitotane [o,p'-DDD or 1,1-(o,p'-dichlorodiphenyl)-2, 2-dichloroethane] has been employed in the nonsurgical treatment of patients with  adrenal carcinoma for several decades. Its use is hampered by serious side  effects, which may be limited by analytically guided dose modifications in the  individual patient. Mitotane analyses have previously been undertaken by gas  chromatography with electron capture detection. A sensitive high-pressure liquid  chromatographic method for measuring mitotane in plasma is described. After  protein precipitation with 1.5 vol of acetone, mitotane and its metabolite  1,1-(o,p'-dichlorodiphenyl)-2,2-dichloroethene (o,p'-DDE) are resolved by  isocratic elution from a C18 reversed-phase support and quantified by ultraviolet  detection at 230 nm. Recoveries of mitotane and o,p'-DDE after deproteinization  were quantitative. Within-run and between-day coefficients of variation were {\textless} 4\%  over the entire therapeutic range. The limit of detection was 0.25 mumol/L and  the standard curve was linear in the 1-100 mumol/L range. The method has been  evaluated using samples obtained from an adolescent girl who had metastatic  adrenocortical carcinoma. Data from this single patient may suggest that systemic  absorption of mitotane is adequate, and toxicity possibly decreased, when  mitotane is administered by the rectal route.},
	language = {eng},
	number = {5},
	journal = {Therapeutic drug monitoring},
	author = {Andersen, A. and Warren, D. J. and Nome, O. and Vesterhus, L. and Slørdal, L.},
	month = oct,
	year = {1995},
	pmid = {8585118},
	note = {Place: United States},
	keywords = {Humans, Female, Adolescent, Absorption, Dichlorodiphenyl Dichloroethylene/*blood, Spectrophotometry, Ultraviolet, Drug Stability, *Chromatography, High Pressure Liquid, Adrenal Cortex Neoplasms/blood/drug therapy, Antineoplastic Agents, Hormonal/administration \& dosage/*blood/therapeutic use, Carcinoma/blood/drug therapy, Mitotane/administration \& dosage/*blood/therapeutic use},
	pages = {526--531},
}

@article{bonin_effect_1985,
	title = {Effect of induction by {DDT} on metabolism and mutagenicity of benzo[a]pyrene.},
