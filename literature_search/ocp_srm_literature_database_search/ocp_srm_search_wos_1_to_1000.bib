@article{ WOS:000360996200005,
Author = {Bachhav, Sagar S. and Kshirsagar, Nilima A.},
Title = {Systematic review of drug utilization studies \& the use of the drug
   classification system in the WHO-SEARO Region},
Journal = {INDIAN JOURNAL OF MEDICAL RESEARCH},
Year = {2015},
Volume = {142},
Pages = {120-129},
Month = {AUG},
Abstract = {Background \& objectives: Information available on drug consumption is
   inadequate in most low and middle income countries. This systematic
   review was conducted to analyse published work on drug utilization
   research/studies (DUR) in the SEARO region of WHO for study objectives,
   methodology, results and recommendations and to identify the need for
   improving DUR and the use of the ATC/DDD system.
   Methods: A literature search for DUR was carried out in biomedical
   databases (PubMed, Scirus, Scopus and Google Scholar) up to May 2012.
   Publications were selected if those were in the English language,
   describing DUR or prescription practices, and study conducted in the
   WHO-SEARO countries.
   Results: A total of 318 publications were included in the review. Of
   these, 67 per cent were from India and 13 per cent were from Thailand.
   Majority of the publications were hospital based; only 16 per cent were
   community based. The ATC/DDD system was used in only 20 per cent of the
   publications, of which 73 per cent publications used DDD indicators.
   Several publications focused on antibiotics (31\%). Publications that
   recommended the need for a policy or intervention to improve
   prescription practices/rational drug use amounted to 35 per cent.
   Interpretation \& conclusions: Drug utilization studies using ATC/DDD
   system need to be promoted and carried out on an ongoing basis. DUR is
   important for rational use of drugs. Its relevance to policy making and
   resource allocation needs to be emphasized.},
DOI = {10.4103/0971-5916.164223},
ISSN = {0971-5916},
ResearcherID-Numbers = {Bachhav, Sagar Sudhakar/ABC-8271-2020},
ORCID-Numbers = {Bachhav, Sagar Sudhakar/0000-0003-0736-2656},
Unique-ID = {WOS:000360996200005},
}

@inproceedings{ WOS:000075846300026,
Author = {Havlicek, SC and Stansbury, J and Ebbs, B},
Editor = {Wickramanayake, GB and Hinchee, RE},
Title = {Intrinsic remediation at former lindane production facility},
Booktitle = {FIRST INTERNATIONAL CONFERENCE ON REMEDIATION OF CHLORINATED AND
   RECALCITRANT COMPOUNDS, VOL 3: NATURAL ATTENUATION},
Year = {1998},
Pages = {153-158},
Note = {1st International Conference on Remediation of Chlorinated and
   Recalcitrant Compounds, MONTEREY, CA, MAY 18-21, 1998},
Abstract = {Lindane was produced at the subject facility for approximately 20 years
   prior to 1966. Historic releases did occur which were confined to the
   site itself. As the fundamental principles of intrinsic remediation (IR)
   became more clearly understood, an ongoing study was initiated in 1997
   to determine if IR might be decontaminating the site and if
   decontamination might be sufficiently rapid to allow consideration of IR
   as a viable alternative to traditional remediation techniques.
   According to literature reports, lindane biodegrades via three major
   pathways:
   Successive replacement of chlorine atoms with hydrogen atoms.
   Successive loss of HCl to form trichlorobenzene followed by replacement
   of the remaining chlorine atoms with hydrogen.
   Oxidation to a chlorinated cyclohexanol, then aromatization to
   chlorinated phenols, then replacement of the remaining chlorine atoms
   with hydrogen.
   In order to gather supporting evidence from on-site analyses, it was
   necessary to both reexamine historic VOA results and to develop methods
   capable of simultaneously determining low levels of as many metabolites
   from the major degradation pathways as was practical. Authentic samples
   of 24 metabolites were obtained and used to confirm that metabolites
   from all three degradation pathways could be determined in a single
   analysis. Metabolites from all degradation pathways were then identified
   in groundwater samples from beneath the facility. In addition many other
   electron-capturing sample components, also believed to be lindane
   metabolites were detected.},
ISBN = {1-57477-058-6},
Unique-ID = {WOS:000075846300026},
}

