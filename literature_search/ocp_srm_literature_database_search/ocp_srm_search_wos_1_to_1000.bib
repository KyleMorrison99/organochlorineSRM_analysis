Pages = {42-46},
Month = {JUN 15},
Abstract = {Reaction of {[}CpFe(CO)(2)I], 1 (Cp = eta(5)-C(5)H(5)) and
   di(organyl)dichalcogenides, E(2)R(2) (E = S, Se; R = Ph, CH(2)Ph,
