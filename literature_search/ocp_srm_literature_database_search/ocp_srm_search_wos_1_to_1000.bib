Unique-ID = {WOS:000322515000009},
}

@article{ WOS:000312618300059,
Author = {Mashroofeh, Abdulreza and Bakhtiari, Alireza Riyahi and Pourkazemi,
   Mohammad and Rasouli, Sana},
Title = {Bioaccumulation of Cd, Pb and Zn in the edible and inedible tissues of
   three sturgeon species in the Iranian coastline of the Caspian Sea},
Journal = {CHEMOSPHERE},
Year = {2013},
Volume = {90},
Number = {2},
Pages = {573-580},
Month = {JAN},
Abstract = {The accumulations of Cd, Pb and Zn were determined in edible and
   inedible tissues of Persian sturgeon (Acipenser persicus; n = 27),
   stellate sturgeon (Acipenser stellatus; n = 5) and beluga (Huso huso; n
   = 4) collected from coastal waters of the South Caspian Sea from March
   to April 2011. Concentrations of metals evaluated in the caviar, muscle,
   liver, kidney, gills, ovary and heart of the three species of sturgeons
   have been assessed using the flame and graphite furnace atomic
   absorption spectrophotometer. Concentrations of Zn, Pb and Cd in the
   edible and inedible tissues were apparently different among the three
   species of sturgeons. Especially, beluga heart showed the highest
   concentrations of Zn and Pb and Cd in Persian sturgeon liver. The
   analyzed metals were found in the caviar and muscle samples of Persian
   sturgeon and likewise muscle samples two other sturgeon species at mean
   concentrations under the permissible limits proposed by MAFF (2000). (C)
   2012 Elsevier Ltd. All rights reserved.},
DOI = {10.1016/j.chemosphere.2012.08.034},
ISSN = {0045-6535},
EISSN = {1879-1298},
ResearcherID-Numbers = {Pourkazemi, Mohammad/C-9510-2017},
