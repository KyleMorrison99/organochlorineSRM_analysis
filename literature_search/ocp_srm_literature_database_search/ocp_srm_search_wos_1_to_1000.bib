@article{ WOS:000381024800005,
Author = {Martinez-Galero, Elizdath and Perez-Pasten, Ricardo and Perez-Juarez,
   Angelica and Fabila-Castillo, Luis and Gutierrez-Salmean, Gabriela and
   Chamorro, German},
Title = {Preclinical antitoxic properties of Spirulina (Arthrospira)},
Journal = {PHARMACEUTICAL BIOLOGY},
Year = {2016},
Volume = {54},
Number = {8},
Pages = {1345-1353},
Month = {AUG},
Abstract = {Context: Spirulina (Arthrospira) exerts a wide spectrum of
   pharmacological activities which are mainly attributed to its
   antioxidant effect. However, Spirulina has also been reported (both in
   preclinical and in clinical scenarios) to exhibit other bioactive
   effects, including an antitoxic potential.
   Objective: We performed a systematic review of the literature, conducted
   in TOXNET, PubMed/MEDLINE, and Science Direct-Scopus; all available
   years were included. Searching criteria included the effects of
   Spirulina on experimental poisonings from arsenic, cadmium, carbon
   tetrachloride, deltamethrin, fluoride, hexachlorocyclohexane, iron,
   lead, lindane, and mercury.
   Results: In all cases, it was established that the blue-green alga, and
   its isolated compounds, effectively counteracted these pollutants toxic
   effects on the exposed organisms. Some molecular mechanisms are
   proposed, although they have not been fully elucidated yet.
   Conclusion: Spirulina could be a useful coadjuvant agent within clinical
   practice for treatment of these or other pollutants poisonings.},
DOI = {10.3109/13880209.2015.1077464},
