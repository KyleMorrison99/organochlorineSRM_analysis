@article{ WOS:000221209100008,
Author = {Bayen, S and Thomas, GO and Lee, HK and Obbard, JP},
Title = {Organochlorine pesticides and heavy metals in green mussel, Perna
   viridis in Singapore},
Journal = {WATER AIR AND SOIL POLLUTION},
Year = {2004},
Volume = {155},
Number = {1-4},
Pages = {103-116},
Month = {JUN},
Abstract = {The green mussel, Perna viridis, was used to measure bioaccumulated
   levels of organochlorine pesticides (OCPs), together with various heavy
   metals, in the marine environment of Singapore. Samples were collected
   from eight different locations in the coastal waters of Singapore
   between April and May 2002. OCPs (i.e. DDT, Chlordane, Mirex,
   hexachlorobenzene (HCB), pentachloronitrobenzene (PCNB) and Heptachlor)
   were quantified by gas chromatography-mass spectrometry. Heavy metals
   (i.e. As, Cd, Cr, Cu, Ni, Pb and Zn) were quantified by induced coupled
   plasma spectrometry. The concentration ranged from 2.6 to 54 ng g(-1) dw
   for DDTs (i.e. sum of p,p'-DDT, p,p'-DDD, o, p'-DDE and p, p'-DDE), 3.1
   to 15 ng g(-1) dw for Chlordanes (i.e. sum of alpha- and
   gamma-chlordane) and 0.26 to 1.5 ng g(-1) dw for Mirex. These levels can
   be considered low when compared to reported values from similar studies
   conducted elsewhere in the world. Other pesticides were not detected.
   All heavy metals analysed were detected in all P. viridis samples, but
   only Cu, Pb, Zn and As were close to existing safety standards. Samples
   taken from locations adjacent to the main shipyards on the coastline and
   busy shipping lanes demonstrated peak levels of heavy metals and
   pesticides in the mussel tissues.},
DOI = {10.1023/B:WATE.0000026524.99553.55},
ISSN = {0049-6979},
EISSN = {1573-2932},
ResearcherID-Numbers = {OBBARD, Jeff/D-4630-2009
   Lee, Hian Kee/B-3993-2018
   },
ORCID-Numbers = {Lee, Hian Kee/0000-0002-5664-9509
   Bayen, Stephane/0000-0002-9935-6685},
Unique-ID = {WOS:000221209100008},
}

