year={2011},
volume={7},
number={6},
pages={323-331},
doi={10.1038/nrneurol.2011.60},
note={cited By 639},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-79958234542&doi=10.1038%2fnrneurol.2011.60&partnerID=40&md5=89ba534f4a1cee4f3305b03677e80e15},
